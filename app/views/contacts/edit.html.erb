<% provide(:title, '更新联系方式') %>
<div class="row">
	<%= render "side" %>
	<div class="span6">
		<%= form_for([@company,@contact]) do |f| %>
			
			<%= f.label :telephone, '座机' %>
			<%= f.text_field :telephone %>

			<%= f.label :mobile, '手机' %>
			<%= f.text_field :mobile %>

			<%= f.label :mail, '邮箱' %>
			<%= f.text_field :mail %>

			<%= f.label :qq, 'QQ' %>
			<%= f.text_field :qq %>

			<%= f.label :other, '其他' %>
			<%= f.text_field :other %>

			<%= f.submit "更新", class: "btn btn-large btn-primary" %>
		<% end %>
	</div>
</div>
