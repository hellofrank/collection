<% provide(:title, '添加工程') %>
<div class="row">
	<%= render "side" %>
	<div class="span6">
    <%= form_for ([@company, @company.projects.build]) do |f| %>

      <%= f.label :name, '工程名' %>
      <%= f.text_field :name %>

      <%= f.label :content, '工程描述' %>
      <%= f.kindeditor :content %>
    <%= f.submit "添加工程", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
